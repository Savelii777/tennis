{
  "name": "tennis-app-backend",
  "version": "1.0.0",
  "description": "Backend for a tennis application built with NestJS",
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc -p tsconfig.build.json",
    "prestart": "npm run build",
    "test:e2e": "jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:watch": "jest --config ./test/jest-e2e.json --runInBand --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "seed": "ts-node src/prisma/seed.ts",
    "start:dev": "ts-node-dev --respawn src/main.ts",
    "start:debug": "LOG_LEVEL=debug ts-node-dev --respawn src/main.ts"
  },
  "dependencies": {
    "@nestjs/common": "11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/platform-express": "11.1.3",
    "@nestjs/swagger": "11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@prisma/client": "^3.15.2",
    "@types/multer": "1.4.13",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-telegram-official": "*",
    "pg": "^8.16.0",
    "prisma": "^3.0.0",
    "rxjs": "^7.0.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.3.24",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@nestjs/testing": "11.1.3",
    "@shelf/jest-dynamodb": "^3.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^27.5.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^7.32.0",
    "jest": "^27.0.0",
    "prettier": "^2.5.1",
    "supertest": "^6.3.4",
    "ts-jest": "^27.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "license": "MIT"
}
